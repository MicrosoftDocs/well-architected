{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml",
          "**/toc.yml",
          "**/index.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.css",
          "**/*.png",
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.gif",
          "**/*.svg",
          "**/*.js",
          "**/*.json",
          "**/*.docx",
          "feed.atom"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/azure/well-architected/breadcrumb/toc.json",
      "feedback_system": "Standard",
      "feedback_github_repo": "MicrosoftDocs/well-architected",
      "brand": "azure",
      "uhfHeaderId": "azure",
      "titleSuffix": "Microsoft Azure Well-Architected Framework",
      "ms.service": "azure-waf",
      "searchScope": [ "Azure", "Microsoft Azure Well-Architected Framework" ]    
     },
    "open_to_public_contributors": {
      "**/*.yml": false
    },
    "fileMetadata": {
      "ms.subservice": {
        "reliability/**/*.*": "waf-pillar-reliability",
        "security/**/*.*": "waf-pillar-security",
        "cost-optimization/**/*.*": "waf-pillar-optimization",
        "performance-efficiency/**/*.*": "waf-pillar-efficiency",
        "operational-excellence/**/*.*": "waf-pillar-excellence",
        "mission-critical/**/*.*": "waf-workload-mission-critical",
        "carrier-grade/**/*.*": "waf-workload-carrier-grade",
        "hybrid/**/*.*": "waf-workload-hybrid",
        "azure-virtual-desktop/**/*.*": "waf-workload-azure-virtual-desktop",
        "azure-vmware/**/*.*": "waf-workload-azure-vmware",        
        "hybrid/**/*.*": "waf-workload-hybrid",        
        "iot/**/*.*": "waf-workload-iot",
        "oracle-iaas/**/*.*": "waf-workload-oracle",        
        "sap/**/*.*": "waf-workload-sap",
        "sustainability/**/*.*": "waf-workload-sustainability",
        "service-guides/**/*.*": "waf-service-guide",
        "ai/**/*.*": "waf-workload-ai",
        "saas/**/*.*": "waf-workload-saas"
      },
      "ms.collection": {
        "ai/**/*.*": "ce-skilling-ai-copilot",
        "saas/**/*.*": "learn-startups"
      }
    },
    "template": [
      "docs.html"
    ],
    "dest": "well-architected",
    "markdownEngineName": "markdig"
  }
}
